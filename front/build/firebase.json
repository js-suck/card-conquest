{
  "hosting": {
    "public": "web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "eu-west1"
    },
    "rewrites": [ {
      "public": "web",
      "destination": "/index.html",
      "source": "**"
    } ]
  }
}